{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Express",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node",
      "args": ["${workspaceFolder}/src/index.ts"],
      "runtimeArgs": ["--inspect-brk=9229"],
      "sourceMaps": true,
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
